{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
        "plugin:react/recommended",
        "airbnb",
		"prettier"
    ],
    "overrides": [
    ],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "plugins": [
        "react"
    ],
    "rules": {
		"react/tsx-filename-extension": [
			"warn",
			{
				"extensions": [".ts", ".tsx"]
			}
		],
		"react/react-in-tsx-scope": "off",
		"comma-dangle": "off"
    }
}
